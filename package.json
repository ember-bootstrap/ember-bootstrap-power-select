{
  "name": "ember-bootstrap-power-select",
  "version": "3.0.1",
  "description": "Integrate ember-power-select into your ember-bootstrap forms",
  "keywords": [
    "ember-addon",
    "ember-bootstrap",
    "ember-power-select"
  ],
  "repository": "https://github.com/kaliber5/ember-bootstrap-power-select.git",
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "ember-cli-babel": "^7.26.5",
    "ember-cli-build-config-editor": "^0.5.0",
    "ember-cli-htmlbars": "^5.7.1",
    "fs-extra": "^9.1.0",
    "rsvp": "^4.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.11.3",
    "ember-bootstrap": "^4.9.0",
    "ember-cli": "~3.21.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-power-select": "^4.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.3",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.30.0",
    "eslint-plugin-ember": "^10.5.1",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.10.0",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
